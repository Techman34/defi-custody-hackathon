ries\":{},\"optimizer\":{\"enabled\":false,\"runs\":200},\"remappings\":[]},\"sources\":{\"/C/Users/ilgiz/IdeaProjects/defi-custody-sf-hackathon/contracts/interfaces/ray/IRAY.sol\":{\"keccak256\":\"0x2eac5f61df38cec21333bc80ef60dc3dd8c127920f98a76628aecfe8f94ac661\",\"urls\":[\"bzzr://1137d3f3b3cc64504d9586ff9cac56737ab905c3e1a645e38a7b237bf819f2a1\"]}},\"version\":1}",
